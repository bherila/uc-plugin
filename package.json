{
  "name": "with-typescript-eslint-jest",
  "author": "@erikdstock",
  "license": "MIT",
  "version": "deploy",
  "scripts": {
    "dev": "next dev --turbopack",
    "postinstall": "prisma generate --sql",
    "build": "yarn graphql:validate && next build",
    "vercel:deploy:prod": "vercel deploy --prod",
    "vercel:deploy:preview": "vercel deploy",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit",
    "graphql:validate": "graphql-codegen --check",
    "graphql:codegen": "graphql-codegen",
    "format": "prettier --write \"src/**/*.{js,ts,tsx}\"",
    "prisma:generate": "prisma generate",
    "prisma:push:preview": "dotenv -e .env.local -- prisma db push",
    "prisma:push:production": "dotenv -e .env.production.local -- prisma db push",
    "lint": "next lint",
    "test": "jest",
    "test-all": "yarn lint && yarn type-check && yarn test",
    "codegen": "graphql-codegen && dotenv -e .env.local -- prisma generate --sql"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@hookform/resolvers": "^3.3.1",
    "@popperjs/core": "^2.11.8",
    "@prisma/client": "^6.5.0",
    "@shopify/shopify-api": "^11.2.0",
    "@testing-library/react": "^14.0.0",
    "@types/lodash": "^4.14.197",
    "@types/node-fetch": "^2.5.8",
    "@types/papaparse": "^5.3.8",
    "@types/react-datepicker": "^4.15.0",
    "@types/underscore": "^1.11.0",
    "animate.css": "^4.1.1",
    "bootstrap": "^5.3.2",
    "currency.js": "^2.0.4",
    "date-fns": "^2.30.0",
    "dotenv": "^8.2.0",
    "husky": "^5.1.3",
    "jest-environment-jsdom": "^29.6.4",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mysql2": "^3.11.4",
    "next": "15.2.6",
    "next-compose-plugins": "^2.2.1",
    "react": "19.0.0",
    "react-bootstrap": "2.10.2",
    "react-datepicker": "^8.2.1",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.46.2",
    "react-perfect-scrollbar": "^1.5.8",
    "react-querybuilder": "^7.7.0",
    "react-router-dom": "^6.28.0",
    "sass": "^1.57.0",
    "server-only": "^0.0.1",
    "serverless-mysql": "^1.5.5",
    "shopify-api-node": "^3.13.4",
    "swr": "^2.3.3",
    "uuid-wand": "^1.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.22.11",
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-resolvers": "^4.5.0",
    "@graphql-codegen/typescript-operations": "^4.4.0",
    "@shopify/api-codegen-preset": "^1.1.8",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/jest": "^29.5.14",
    "@types/react": "19.0.12",
    "@types/testing-library__react": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "babel-jest": "^26.6.3",
    "class-validator": "^0.14.1",
    "encoding": "^0.1.13",
    "eslint": "^8.48.0",
    "eslint-config-next": "15.2.4",
    "graphql": "^16.10.0",
    "graphql-codegen-cli": "^0.6.0-alpha.62b22f38",
    "jest": "^29.7.0",
    "lint-staged": "^10.5.4",
    "prettier": "^3.0.3",
    "prettier-plugin-sql": "^0.18.1",
    "prisma": "^6.5.0",
    "reflect-metadata": "^0.2.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "5.1.3",
    "webpack": "^5.88.2",
    "zod-prisma-types": "^3.2.4"
  },
  "resolutions": {
    "@types/react": "19.0.12"
  }
}
